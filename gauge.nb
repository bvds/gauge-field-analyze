Cell["Project into dynamic (non-gauge-transform) subspace", \
"Subsection",ExpressionUUID->"8ea74860-28d0-4be0-bf07-ef3aa73dd5c4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "hess", ",", "grad", ",", "proj", ",", "shifts", ",", "delta", ",", 
      RowBox[{"rl", "=", 
       RowBox[{"makeRootLattice", "[", "]"}]}], ",", 
      RowBox[{"fDir", "=", "0"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{
      RowBox[{"{", 
       RowBox[{"hess", ",", "grad"}], "}"}], "=", 
      RowBox[{"latticeHessian", "[", 
       RowBox[{
        RowBox[{"getLink", "[", "rl", "]"}], ",", 
        RowBox[{"fixedDir", "\[Rule]", "fDir"}], ",", 
        RowBox[{"fullMatrix", "\[Rule]", "True"}]}], "]"}]}], ";", 
     RowBox[{"proj", "=", 
      RowBox[{"NullSpace", "[", 
       RowBox[{"gaugeTransformShifts", "[", 
        RowBox[{"rl", ",", "fDir"}], "]"}], "]"}]}], ";", 
     RowBox[{"projHess0", "=", 
      RowBox[{"proj", ".", "hess", ".", 
       RowBox[{"Transpose", "[", "proj", "]"}]}]}], ";", 
     RowBox[{"projGrad0", "=", 
      RowBox[{"proj", ".", "grad"}]}], ";", 
     RowBox[{"proj0", "=", "proj"}], ";", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"valuesd", ",", "ood"}], "}"}], "=", 
      RowBox[{"Map", "[", 
       RowBox[{"Reverse", ",", 
        RowBox[{"Eigensystem", "[", "projHess0", "]"}]}], "]"}]}], ";", 
     RowBox[{"applyCutoff3", "[", 
      RowBox[{"valuesd", ",", 
       RowBox[{"ood", ".", "projGrad0"}], ",", 
