        RowBox[{"randomSUMatrix", "[", "4", "]"}]}], ",", "ur"}], "}"}], ",", 
     
     RowBox[{
      RowBox[{"ur", "=", 
       RowBox[{"SUPower", "[", 
        RowBox[{"u", ",", "0.5", ",", 
         RowBox[{"\"\<debug\>\"", "\[Rule]", "True"}]}], "]"}]}], ";", 
      RowBox[{"matrixEqual", "[", 
       RowBox[{
        RowBox[{"ur", ".", "ur"}], ",", "u", ",", 
