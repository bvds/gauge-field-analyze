Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "hess", ",", "grad", ",", "proj", ",", "shifts", ",", "delta", ",", 
      RowBox[{"rl", "=", 
