Cell["Alternatively, calculate new value and save :", \
"Text",ExpressionUUID->"aa9caaf6-8234-48da-86c8-2a0ce7516e1d"],

Cell[BoxData[
 RowBox[{"ColumnForm", "[", 
  RowBox[{"stringModelValues", "=", 
   RowBox[{"Table", "[", 
    RowBox[{
     RowBox[{"Block", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"gaugeField", ",", " ", "tallyData", ",", "ff"}], "}"}], ",", 
       
       RowBox[{
        RowBox[{"Get", "[", 
         RowBox[{"\"\<../data/3-3/periodic-20-28-\>\"", "<>", 
          RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<.m\>\""}], "]"}], 
        ";", 
        RowBox[{"tallyData", "=", 
         RowBox[{"talliesToAverageErrors", "[", 
          RowBox[{"polyakovCorrelatorTallies", "[", 
           RowBox[{"\"\<simple\>\"", ",", "\"\<1\>\""}], "]"}], "]"}]}], ";", 
        
        RowBox[{"ff", "=", 
         RowBox[{"stringModel", "[", 
          RowBox[{"tallyData", ",", 
           RowBox[{"printResult", "\[Rule]", "True"}], ",", 
           RowBox[{"\"\<lowerCutoff\>\"", "\[Rule]", "40"}]}], "]"}]}], ";", 
        RowBox[{"Map", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"valueError", "[", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "1", "]"}], "]"}], ",", 
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}]}], "]"}], "&"}], ",", 
          RowBox[{"ff", "[", "\"\<ParameterTableEntries\>\"", "]"}]}], 
         "]"}]}]}], "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"i", ",", "1", ",", "30"}], "}"}]}], "]"}]}], 
  "]"}]], "Input",ExpressionUUID->"8d6fb49d-ee75-4f0a-9441-d7626fed9fb4"],

Cell[BoxData[
 RowBox[{"ListPlot", "[", 
  RowBox[{
   RowBox[{"Map", "[", 
    RowBox[{
     RowBox[{
      RowBox[{"Transpose", "[", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Range", "[", 
          RowBox[{"0", ",", "10"}], "]"}], ",", 
         RowBox[{"CorrelationFunction", "[", 
          RowBox[{
           RowBox[{"Map", "[", 
            RowBox[{"First", ",", "#"}], "]"}], ",", 
           RowBox[{"{", "10", "}"}]}], "]"}]}], "}"}], "]"}], "&"}], ",", 
     RowBox[{"Transpose", "[", 
      RowBox[{"Select", "[", 
       RowBox[{"stringModelValues", ",", " ", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{
            RowBox[{"Head", "[", "#", "]"}], "===", "List"}], "&&", 
           RowBox[{"NumberQ", "[", 
