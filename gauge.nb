Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"applyCutoff", "::", "usage"}], "=", 
   "\"\<Large zzz and damping=1 removes the cutoff.  When shift=pi, there is \
an inflection point in the associated color matrix meaning that the quadratic \
approximation becomes qualitatively different than the full matrix expential. \
 Thus we set shifts>pi to zero.  The cutoff represents the point where we \
believe the quadratic approximation starts breaking down.\>\""}], ";", 
  RowBox[{
   RowBox[{"applyCutoff", "[", 
    RowBox[{"cutoff_", ",", 
     RowBox[{"zzz_:", "1"}]}], "]"}], ":=", 
   RowBox[{
    RowBox[{"Which", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"Pi", " ", "zzz", "  ", 
        RowBox[{"Abs", "[", "#1", "]"}]}], "<=", 
       RowBox[{"Abs", "[", "#2", "]"}]}], ",", "0", ",", 
      RowBox[{
       RowBox[{"Abs", "[", "#2", "]"}], "<", 
       RowBox[{"cutoff", " ", 
        RowBox[{"Abs", "[", "#1", "]"}], " ", "zzz"}]}], ",", " ", 
      RowBox[{"#2", "/", "#1"}], ",", "True", ",", " ", 
      RowBox[{
       RowBox[{"Sign", "[", 
        RowBox[{"#1", " ", "#2"}], "]"}], " ", "cutoff", " ", "zzz", " ", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{
           RowBox[{"Abs", "[", "#2", "]"}], "/", 
           RowBox[{"(", 
            RowBox[{"zzz", " ", "Pi", " ", 
             RowBox[{"Abs", "[", "#1", "]"}]}], ")"}]}]}], ")"}], "/", 
        RowBox[{"(", 
         RowBox[{"1", "-", 
          RowBox[{"cutoff", "/", "Pi"}]}], ")"}]}]}]}], "]"}], 
    "&"}]}]}]], "Input",
 InitializationCell->
  True,ExpressionUUID->"5d7f8e6d-f4d8-4eb8-9b13-e5badac257f5"],

