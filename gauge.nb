 RowBox[{
  RowBox[{"corr", "=", 
   RowBox[{"Block", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{
       RowBox[{"data", "=", 
        RowBox[{"Values", "[", "polyakovCorrelatorValues", "]"}]}], ",", 
       "cov", ",", "dd"}], "}"}], ",", "\[IndentingNewLine]", 
     RowBox[{"(*", " ", 
      RowBox[{"Construct", " ", "full", " ", "covariance", " ", "matrix"}], 
      " ", "*)"}], "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"cov", "=", 
       RowBox[{"Outer", "[", 
        RowBox[{"Covariance", ",", "data", ",", "data", ",", "1"}], "]"}]}], 
      ";", 
      RowBox[{"dd", "=", 
       RowBox[{"Sqrt", "[", 
