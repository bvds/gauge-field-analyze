 RowBox[{
  RowBox[{
   RowBox[{"randomDirection", "[", "]"}], ":=", 
   RowBox[{
    RowBox[{"RandomVariate", "[", 
     RowBox[{"CircularRealMatrixDistribution", "[", 
      RowBox[{
       RowBox[{"nc", "^", "2"}], "-", "1"}], "]"}], "]"}], "[", 
    RowBox[{"[", "1", "]"}], "]"}]}], 
  ";"}]}], "Input",ExpressionUUID->"31f14934-c218-4714-a133-04f2e576e946"],

