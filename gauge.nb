 RowBox[{
  RowBox[{"makeTrajectory", "[", 
   RowBox[{"label_", ",", "n_", ",", 
    RowBox[{"fixed_:", "0"}]}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "gaugeField", ",", "tallyData", ",", "ff", ",", "gaugeField0", ",", 
      "distance"}], "}"}], ",", 
    RowBox[{
     RowBox[{"<<", "\"\<../data/3-4/periodic-16-48-5.m\>\""}], ";", 
     RowBox[{
      RowBox[{"trajectory", "[", "label", "]"}], "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"Print", "[", 
          RowBox[{"\"\<Starting \>\"", ",", " ", "i"}], "]"}], ";", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{"i", ">", "0"}], ",", 
           RowBox[{
            RowBox[{"Get", "[", 
             RowBox[{
             "\"\<../data/3-4/step-16-48-5-\>\"", "<>", "label", "<>", 
              "\"\<-\>\"", "<>", 
              RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<.m\>\""}], 
             "]"}], ";", "\[IndentingNewLine]", 
            RowBox[{"gaugeField", "=", "gaugeField0"}]}]}], "]"}], ";", 
