Cell["Calculate normal trajectory and save in file", \
"Subsubsection",ExpressionUUID->"067fa35f-5347-4dc2-90fc-2442160c00ea"],

Cell[BoxData[
 RowBox[{
  RowBox[{"makeTrajectory", "[", 
   RowBox[{"label_", ",", "n_", ",", 
    RowBox[{"fixed_:", "0"}]}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
     "gaugeField", ",", "tallyData", ",", "ff", ",", "gaugeField0", ",", 
      "distance"}], "}"}], ",", 
    RowBox[{
     RowBox[{"<<", "\"\<../data/3-3/periodic-16-28-5.m\>\""}], ";", 
     RowBox[{"gi", "=", "gaugeField"}], ";", 
     RowBox[{
      RowBox[{"trajectory", "[", "label", "]"}], "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"Print", "[", 
         RowBox[{"\"\<Starting \>\"", ",", " ", "i"}], "]"}], ";", 
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", ">", "0"}], ",", 
          RowBox[{
           RowBox[{"Get", "[", 
            RowBox[{
            "\"\<../data/3-3/step-16-28-5-\>\"", "<>", "label", "<>", 
             "\"\<-\>\"", "<>", 
             RowBox[{"ToString", "[", "i", "]"}], "<>", "\"\<.m\>\""}], "]"}],
            ";", "\[IndentingNewLine]", 
           RowBox[{"tallyData", "=", 
            RowBox[{"talliesToAverageErrors", "[", 
             RowBox[{"polyakovLoopTallies", "[", "\"\<smeared\>\"", "]"}], 
             "]"}]}], ";", 
           RowBox[{"ff", "=", 
            RowBox[{"exponentialModel", "[", "tallyData", "]"}]}], ";", 
           RowBox[{"Join", "[", 
            RowBox[{
             RowBox[{"{", 
              RowBox[{"i", ",", 
               RowBox[{"averagePlaquette", "[", "]"}]}], "}"}], ",", 
             RowBox[{"Inner", "[", 
              RowBox[{
               RowBox[{
                RowBox[{"{", 
                 RowBox[{
                  RowBox[{"#1", "[", 
                   RowBox[{"[", "1", "]"}], "]"}], ",", 
                  RowBox[{"#1", "[", 
                   RowBox[{"[", "2", "]"}], "]"}], ",", "#2"}], "}"}], "&"}], 
               ",", 
               RowBox[{"ff", "[", "\"\<BestFitParameters\>\"", "]"}], ",", 
               RowBox[{"ff", "[", "\"\<ParameterErrors\>\"", "]"}], ",", 
               "List"}], "]"}]}], "]"}]}], ",", 
          RowBox[{"{", 
           RowBox[{"i", ",", "0", ",", "n"}], "}"}]}], "]"}]}], 
       "]"}]}]}]}]}]}]], \
"Input",ExpressionUUID->"fc7c8716-57dc-4020-9e3c-fb3a9c83dd11"],

