          RowBox[{"Tr", "[", 
           RowBox[{"u", ".", "u", ".", "#1", ".", 
            RowBox[{"ConjugateTranspose", "[", 
             RowBox[{"u", ".", "u"}], "]"}], ".", "#2"}], "]"}]}], "&"}], ",", 
        RowBox[{"SUGenerators", "[", "]"}], ",", 
        RowBox[{"SUGenerators", "[", "]"}], ",", "1"}], "]"}], "]"}]}], 
    ";"}]}], "]"}]], \
