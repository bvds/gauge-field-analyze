           RowBox[{"u", ".", 
            RowBox[{"getLink", "[", 
             RowBox[{"dir", ",", "coords"}], "]"}]}]}], "]"}], ";", 
         "\[IndentingNewLine]", 
         RowBox[{"setLink", "[", 
          RowBox[{"dir", ",", 
