        RowBox[{"getLink", "[", "rl", "]"}], ",", 
        RowBox[{"fixedDir", "\[Rule]", "fDir"}], ",", 
        RowBox[{"fullMatrix", "\[Rule]", "True"}]}], "]"}]}], ";", 
     RowBox[{"proj", "=", 
      RowBox[{"NullSpace", "[", 
       RowBox[{"gaugeTransformShifts", "[", 
        RowBox[{"rl", ",", "fDir"}], "]"}], "]"}]}], ";", 
     RowBox[{"projHess0", "=", 
      RowBox[{"proj", ".", "hess", ".", 
       RowBox[{"Transpose", "[", "proj", "]"}]}]}], ";", 
     RowBox[{"projGrad0", "=", 
      RowBox[{"proj", ".", "grad"}]}], ";", 
     RowBox[{"proj0", "=", "proj"}], ";", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"valuesd", ",", "ood"}], "}"}], "=", 
      RowBox[{"Map", "[", 
       RowBox[{"Reverse", ",", 
        RowBox[{"Eigensystem", "[", "projHess0", "]"}]}], "]"}]}], ";", 
     RowBox[{"applyCutoff3", "[", 
      RowBox[{"valuesd", ",", 
       RowBox[{"ood", ".", "projGrad0"}], ",", 
