Cell["\<\
Independent test of the saddle-lib program using an explicitly constructed \
Hessian.  We use blockSize=1, since blockSize has been well-vetted.  Also, \
the hessian matrix is dense.\
\>", "Text",
 CellChangeTimes->{{3.782560899417079*^9, 3.7825610135091877`*^9}, {
  3.7825836769597282`*^9, 
  3.782583715750764*^9}},ExpressionUUID->"b49dc30d-f685-4917-888e-\
8925caca7789"],

Cell[BoxData[
 RowBox[{
  RowBox[{"makeZeroSaddle", "[", 
   RowBox[{"constraints_", ",", "largeShifts_", ",", "nTotal_"}], "]"}], ":=", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"n1", "=", 
       RowBox[{"nTotal", "-", "largeShifts", "-", "constraints"}]}], ",", 
      RowBox[{"large", "=", "4"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"hess", "=", 
      RowBox[{"DiagonalMatrix", "[", 
       RowBox[{"Join", "[", 
        RowBox[{
         RowBox[{"Table", "[", 
          RowBox[{"0", ",", 
           RowBox[{"{", "constraints", "}"}]}], "]"}], ",", 
         RowBox[{"RandomReal", "[", 
          RowBox[{
           RowBox[{
            RowBox[{"{", 
             RowBox[{
              RowBox[{"-", "1"}], ",", "1"}], "}"}], "/", "large"}], ",", 
           "largeShifts"}], "]"}], ",", 
         RowBox[{"RandomReal", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{
             RowBox[{"-", "1"}], ",", "1"}], "}"}], " ", ",", 
           RowBox[{"nTotal", "-", "constraints", "-", "largeShifts"}]}], 
          "]"}]}], "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"gauge", "=", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{"i", "\[Equal]", "j"}], ",", "1", ",", "0"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "constraints"}], "}"}], ",", 
        RowBox[{"{", 
         RowBox[{"j", ",", "nTotal"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"x0", " ", "=", " ", 
      RowBox[{"Table", "[", 
       RowBox[{
        RowBox[{"If", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"i", ">", "constraints"}], "&&", 
           RowBox[{"i", "<=", 
            RowBox[{"constraints", "+", "largeShifts"}]}]}], ",", "large", 
          ",", "1"}], "]"}], ",", 
        RowBox[{"{", 
         RowBox[{"i", ",", "nTotal"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"grad", "=", 
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"x0", ".", "hess"}], "+", 
         RowBox[{"hess", ".", "x0"}]}], ")"}], "/", "2"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"nc", "=", 
      RowBox[{"Sqrt", "[", "2", "]"}]}], ";", 
     RowBox[{"latticeDimensions", "=", 
      RowBox[{"{", 
       RowBox[{"GCD", "[", 
        RowBox[{"constraints", ",", "nTotal"}], "]"}], "}"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"oo", "=", 
      RowBox[{"RandomVariate", "[", 
       RowBox[{"CircularRealMatrixDistribution", "[", "nTotal", "]"}], 
       "]"}]}], ";"}]}], "]"}]}]], "Input",
