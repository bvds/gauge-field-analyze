Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"pointPotential2", "::", "usage"}], "=", 
   "\"\<Return the potential for a point charge in 2 dimensions, calculated \
using the lowest order lattice discretization.  Set r=1 to zero potential.  \
This was create by Mma notebook \\\"poisson-equation.nb\\\"\>\""}], ";", 
  RowBox[{
   RowBox[{"pointPotential2", "::", "cutoff"}], "=", 
   "\"\<Coordinates `1` exceed the cutoff `2`\>\""}], ";", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"of", "=", "\"\<mma-lib/point-potential2.m\>\""}], ",", 
      RowBox[{"cutoff", "=", "50"}], ",", 
      RowBox[{"v0", "=", 
       RowBox[{"vv", "[", 
        RowBox[{"[", 
         RowBox[{"index", "[", 
          RowBox[{"{", 
           RowBox[{"1", ",", "0"}], "}"}], "]"}], "]"}], "]"}]}]}], "}"}], 
    ",", 
    RowBox[{
     RowBox[{"pointPotential2Data", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"{", "}"}], "]"}]}], ";", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Norm", "[", 
           RowBox[{"coords", "[", "k", "]"}], "]"}], "\[LessEqual]", 
          "cutoff"}], ",", 
         RowBox[{
          RowBox[{"pointPotential2Data", "[", 
           RowBox[{"coords", "[", "k", "]"}], "]"}], "=", 
          RowBox[{
           RowBox[{"-", 
            RowBox[{"vv", "[", 
             RowBox[{"[", "k", "]"}], "]"}]}], "+", "v0"}]}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", 
         RowBox[{"Length", "[", "vv", "]"}]}], "}"}]}], "]"}], ";", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"pointPotential2", "[", 
          RowBox[{
           RowBox[{"i_", "?", "NumericQ"}], ",", 
           RowBox[{"j_", "?", "NumericQ"}]}], "]"}], ":=", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"i", "^", "2"}], "+", 
             RowBox[{"j", "^", "2"}]}], ">", 
            RowBox[{"#", "^", "2"}]}], ",", 
           RowBox[{
            RowBox[{"Message", "[", 
             RowBox[{
              RowBox[{"pointPotential2", "::", "cutoff"}], ",", 
              RowBox[{"{", 
               RowBox[{"i", ",", "j"}], "}"}], ",", "#"}], "]"}], ";", 
            "$Failed"}], ",", 
           RowBox[{"pointPotential2Data", "[", 
            RowBox[{"Sort", "[", 
             RowBox[{
              RowBox[{"Round", "[", 
               RowBox[{"Abs", "[", 
                RowBox[{"{", 
                 RowBox[{"i", ",", "j"}], "}"}], "]"}], "]"}], ",", 
              "Greater"}], "]"}], "]"}]}], "]"}]}], ")"}], "&"}], "[", 
      "cutoff", "]"}], ";", 
     RowBox[{"DeleteFile", "[", "of", "]"}], ";", 
     RowBox[{"Save", "[", 
      RowBox[{"of", ",", 
       RowBox[{"{", "\"\<pointPotential2\>\"", "}"}]}], "]"}]}]}], 
   "]"}]}]], "Input",
