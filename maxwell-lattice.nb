Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"pointPotential3", "::", "usage"}], "=", 
   "\"\<Return the potential for a point charge in 3 dimensions, calculated \
using the lowest order lattice discretization.  This was create by Mma \
notebook \\\"poisson-equation.nb\\\"\>\""}], ";", 
  RowBox[{
   RowBox[{"pointPotential3", "::", "cutoff"}], "=", 
   "\"\<Coordinates `1` exceed the cutoff `2`\>\""}], ";", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"of", "=", "\"\<mma-lib/point-potential3.m\>\""}], ",", 
      RowBox[{"cutoff", "=", "35"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"pointPotential3Data", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"{", "}"}], "]"}]}], ";", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Norm", "[", 
           RowBox[{"coords", "[", "k", "]"}], "]"}], "\[LessEqual]", 
          "cutoff"}], ",", 
         RowBox[{
          RowBox[{"pointPotential3Data", "[", 
           RowBox[{"coords", "[", "k", "]"}], "]"}], "=", 
          RowBox[{"vv", "[", 
