Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"pointPotential3", "::", "usage"}], "=", 
   "\"\<Return the potential for a point charge in 3 dimensions, calculated \
using the lowest order lattice discretization.  This was create by Mma \
notebook \\\"maxwell-lattice.nb\\\"\>\""}], ";", 
  RowBox[{
   RowBox[{"pointPotential3", "::", "cutoff"}], "=", 
   "\"\<Coordinates `1` exceed the cutoff `2`\>\""}], ";", 
  RowBox[{"Block", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{
      RowBox[{"of", "=", "\"\<mma-lib/point-potential3.m\>\""}], ",", 
      RowBox[{"cutoff", "=", "35"}]}], "}"}], ",", 
    RowBox[{
     RowBox[{"pointPotential3Data", "=", 
      RowBox[{"Association", "[", 
       RowBox[{"{", "}"}], "]"}]}], ";", 
     RowBox[{"Do", "[", 
      RowBox[{
       RowBox[{"If", "[", 
        RowBox[{
         RowBox[{
          RowBox[{"Norm", "[", 
           RowBox[{"coords", "[", "k", "]"}], "]"}], "\[LessEqual]", 
          "cutoff"}], ",", 
         RowBox[{
          RowBox[{"pointPotential3Data", "[", 
           RowBox[{"coords", "[", "k", "]"}], "]"}], "=", 
          RowBox[{"vv", "[", 
           RowBox[{"[", "k", "]"}], "]"}]}]}], "]"}], ",", 
       RowBox[{"{", 
        RowBox[{"k", ",", 
         RowBox[{"Length", "[", "vv", "]"}]}], "}"}]}], "]"}], ";", 
     RowBox[{
      RowBox[{
       RowBox[{"(", 
        RowBox[{
         RowBox[{"pointPotential3", "[", 
          RowBox[{
           RowBox[{"x_", "?", "NumericQ"}], ",", 
           RowBox[{"y_", "?", "NumericQ"}], ",", 
           RowBox[{"z_", "?", "NumericQ"}]}], "]"}], ":=", 
         RowBox[{"If", "[", 
          RowBox[{
           RowBox[{
            RowBox[{
             RowBox[{"x", "^", "2"}], "+", 
             RowBox[{"y", "^", "2"}], "+", 
             RowBox[{"z", "^", "2"}]}], ">", 
            RowBox[{"#", "^", "2"}]}], ",", 
           RowBox[{
            RowBox[{"Message", "[", 
             RowBox[{
              RowBox[{"pointPotential3", "::", "cutoff"}], ",", 
              RowBox[{"{", 
               RowBox[{"x", ",", "y", ",", "z"}], "}"}], ",", "#"}], "]"}], 
            ";", "$Failed"}], ",", 
           RowBox[{"pointPotential3Data", "[", 
            RowBox[{"Sort", "[", 
             RowBox[{
              RowBox[{"Round", "[", 
               RowBox[{"Abs", "[", 
                RowBox[{"{", 
                 RowBox[{"x", ",", "y", ",", "z"}], "}"}], "]"}], "]"}], ",", 
              "Greater"}], "]"}], "]"}]}], "]"}]}], ")"}], "&"}], "[", 
      "cutoff", "]"}], ";", 
     RowBox[{"DeleteFile", "[", "of", "]"}], ";", 
     RowBox[{"Save", "[", 
      RowBox[{"of", ",", 
       RowBox[{"{", "\"\<pointPotential3\>\"", "}"}]}], "]"}]}]}], 
   "]"}]}]], "Input",
